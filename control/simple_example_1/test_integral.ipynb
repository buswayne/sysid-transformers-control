   ],
   "source": [
    "# integral\n",
    "v = np.cumsum(u)\n",
    "# derivative\n",
    "u_bis = np.diff(v, prepend=0)\n",
    "# plt.plot(v)\n",
    "plt.plot(u)\n",
    "plt.plot(u_bis)\n"
   ],
